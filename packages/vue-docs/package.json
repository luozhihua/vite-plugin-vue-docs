{
  "name": "vite-plugin-vue-docs",
  "version": "0.1.9",
  "description": "vite plugin auto document generation for vue",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "dev": "yarn tsup --watch & yarn watch:template",
    "build": "rm -rf dist && yarn tsup && yarn test && yarn cp:template && yarn create:vue",
    "create:vue": "node scripts/md.js",
    "tsup": "tsup src/index.ts --dts",
    "watch:template": "nodemon --watch ./src/template -e vue,css --exec 'yarn cp:template && yarn create:vue'",
    "cp:template": "copyfiles -f src/template/* dist/template",
    "test:dev": "jest --watch",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetqy/vite-plugin-vue-docs.git"
  },
  "keywords": [
    "vue",
    "vite",
    "vite-plugin"
  ],
  "author": "meetqy <meetqy@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetqy/vite-plugin-vue-docs/issues"
  },
  "homepage": "https://github.com/meetqy/vite-plugin-vue-docs#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/babel__traverse": "^7.14.0",
    "@types/cheerio": "^0.22.30",
    "@types/debug": "^4.1.6",
    "@types/fs-extra": "^9.0.12",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "babel-jest": "^27.0.6",
    "copyfiles": "^2.4.1",
    "debug": "^4.3.2",
    "fs-extra": "^10.0.0",
    "highlight.js": "^10.3.2",
    "jest": "^27.0.6",
    "markdown-it": "^12.1.0",
    "nodemon": "^2.0.9",
    "tsup": "^4.12.0",
    "typescript": "^4.3.4",
    "vite": "^2.3.8"
  },
  "peerDependencies": {
    "vite": "^2.3.8",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "@babel/traverse": "^7.14.7",
    "@babel/types": "^7.14.5",
    "@vue/compiler-sfc": "^3.1.2",
    "cheerio": "^1.0.0-rc.10",
    "comment-parser": "^1.1.5",
    "fast-glob": "^3.2.7",
    "humps": "^2.0.1",
    "vue-router": "4",
    "vue3-highlightjs": "^1.0.5"
  },
  "jest": {
    "coverageReporters": [
      "json-summary"
    ]
  },
  "gitHead": "90b534a94285514ff78ccb58ecabd24f14a0a2f8"
}
